{
  "checks": [
    {
      "id": "DNS OUTBOUND",
      "in_intf": "inside",
      "proto": "udp",
      "src_ip": "203.0.113.2",
      "src_port": 5000,
      "dst_ip": "8.8.8.8",
      "dst_port": 53,
      "should": "allow"
    },
    {
      "id": "HTTPS OUTBOUND",
      "in_intf": "inside",
      "proto": "tcp",
      "src_ip": "203.0.113.2",
      "src_port": 5000,
      "dst_ip": "20.0.0.1",
      "dst_port": 443,
      "should": "drop"
    },
    {
      "id": "SSH INBOUND",
      "in_intf": "management",
      "proto": "tcp",
      "src_ip": "fc00:172:31:2::a",
      "src_port": 5000,
      "dst_ip": "fc00:203:0:113::2",
      "dst_port": 22,
      "should": "drop"
    },
    {
      "id": "PING OUTBOUND",
      "in_intf": "inside",
      "proto": "icmp",
      "src_ip": "203.0.113.2",
      "icmp_type": 8,
      "icmp_code": 0,
      "dst_ip": "8.8.8.8",
      "should": "allow"
    },
    {
      "id": "L2TP OUTBOUND",
      "in_intf": "inside",
      "proto": 115,
      "src_ip": "203.0.113.2",
      "dst_ip": "20.0.0.1",
      "should": "drop"
    }
  ]
}
